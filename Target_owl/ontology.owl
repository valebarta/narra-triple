    </owl:disjointWith>
    <rdfs:label xml:lang="en">E18 Physical Thing</rdfs:label>
    <skos:notation>E18</skos:notation>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://erlangen-crm.org/current/P53_has_former_or_current_location"/>
        </owl:onProperty>
