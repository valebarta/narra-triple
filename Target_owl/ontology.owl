                    <rdfs:label>Election of Boniface VIII</rdfs:label>
                    <crm:P1_is_identified_by>
                      <crm:E41_Appellation rdf:about="https://dlnarratives.eu/string/Election_of_Boniface_VIII">
                        <crm:P3_has_note>Election of Boniface VIII</crm:P3_has_note>
