  <owl:ObjectProperty rdf:about="http://erlangen-crm.org/current/P114_is_equal_in_time_to">
    <rdfs:label xml:lang="en">P114 is equal in time to</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >P114</skos:notation>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E2_Temporal_Entity"/>
