        rdfs:domain         crm:E89_Propositional_Object ;
        rdfs:label          "P129 is about"@en ;
        rdfs:range          crm:E1_CRM_Entity ;
        rdfs:subPropertyOf  crm:P67_refers_to ;
        owl:inverseOf       crm:P129i_is_subject_of ;
        skos:notation       "P129"^^xsd:string .

